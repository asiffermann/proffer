{
  "Storage": {
    "Providers": {
      "Basic": {
        "Type": "FileSystem"
      },
      "CustomRootPath": {
        "Type": "FileSystem",
        "RootPath": "../FileVault2"
      }
    },

    "Stores": {
      "Basic": {
        "ProviderType": "FileSystem"
      },
      "NamedProvider": {
        "ProviderName": "Basic"
      },
      "CustomRootPathNamedProvider": {
        "ProviderName": "CustomRootPath"
      },
      "CustomFolderName": {
        "ProviderType": "FileSystem",
        "FolderName": "Custom"
      },
      "CustomRootPath": {
        "ProviderType": "FileSystem",
        "RootPath": "../CustomRootPath"
      },
      "CustomRootPathWithFolder": {
        "ProviderName": "CustomRootPath",
        "RootPath": "../CustomRootPathWithFolder",
        "FolderName": "InsideFolder"
      }
    },

    "ScopedStores": {
      "ScopedBasic": {
        "ProviderType": "FileSystem",
        "FolderNameFormat": "Basic-{0}"
      },
      "ScopedNamedProvider": {
        "ProviderName": "Basic",
        "FolderNameFormat": "NamedProvider-{0}"
      },
      "ScopedCustomRootPathNamedProvider": {
        "ProviderName": "CustomRootPath",
        "FolderNameFormat": "CustomRootPathNamedProvider-{0}"
      },
      "ScopedCustomRootPath": {
        "ProviderType": "FileSystem",
        "RootPath": "../CustomRootPathWithFolder",
        "FolderNameFormat": "ScopedCustomRootPath-{0}"
      }
    }
  }
}
